{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": [
            "scripts/**/*.js",
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js"
        ]
    },
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-bitwise": false,
        "no-debugger": false
    },
    "jsRules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-bitwise": false,
        "no-debugger": false
    }
}
